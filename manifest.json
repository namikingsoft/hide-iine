{
  "manifest_version": 2,
  "name": "Hide IINE",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"],
      "css": ["embeded.css"],
      "run_at" : "document_start"
    }
  ],
  "web_accessible_resources": [
    "embeded.js",
    "embeded.css"
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
